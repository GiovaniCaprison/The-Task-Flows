{
  "name": "@team10/ui",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "sideEffects": false,
  "engines": {
    "node": ">=20.0.0"
  },
  "files": [
    "build",
    "../../build/storybook",
    "../../build/ui"
  ],
  "dependencies": {
    "@emotion/react": "^11.11.3",
    "@emotion/styled": "^11.11.0",
    "@fontsource/roboto": "^5.0.8",
    "@mui/icons-material": "^5.15.16",
    "@mui/material": "^5.15.16",
    "@mui/system": "^5.15.15",
    "@mui/x-data-grid": "^7.7.1",
    "@tanstack/react-query": "^4.36.1",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^14.3.1",
    "@testing-library/user-event": "^13.5.0",
    "@trpc/client": "^10.45.0",
    "@trpc/react-query": "^10.45.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^20.0.0",
    "aws-rum-web": "^1.16.1",
    "buffer": "^6.0.3",
    "jwks-rsa": "^3.1.0",
    "markdown-to-jsx": "^7.4.0",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-error-boundary": "^3.1.4",
    "react-router-dom": "^6.22.2",
    "react-scripts": "5.0.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@aws-sdk/protocol-http": "^3.374.0",
    "@aws-sdk/types": "^3.489.0",
    "@storybook/addon-a11y": "^7.6.15",
    "@storybook/addon-essentials": "^7.6.15",
    "@storybook/addon-themes": "^7.6.15",
    "@storybook/builder-vite": "^7.6.15",
    "@storybook/react": "^7.6.15",
    "@storybook/react-vite": "^7.6.15",
    "@storybook/types": "^7.6.15",
    "@team10/api": "*",
    "@testing-library/react": "^14.1.2",
    "@types/react": "^18.2.47",
    "@types/react-dom": "^18.2.18",
    "@vitejs/plugin-react-swc": "^3.6.0",
    "jsdom": "^23.2.0",
    "storybook": "^7.6.15"
  },
  "scripts": {
    "build": "vite build",
    "build:storybook": "storybook build --output-dir ../../build/storybook",
    "clean": "rm -rf build coverage node_modules",
    "fix": "prettier --write \"./**/*.{js,ts,tsx}\" && eslint --fix \"./**/*.{js,ts,tsx}\"",
    "lint": "eslint \"./**/*.{js,ts,tsx}\"",
    "start": "vite dev",
    "start:storybook": "storybook dev --port 9009",
    "test": "vitest run",
    "test:update-snapshot": "vitest run --update",
    "type-check": "tsc"
  },
  "nx": {
    "targets": {
      "build": {
        "dependsOn": [
          "build:storybook"
        ],
        "outputs": [
          "{workspaceRoot}/build/ui"
        ]
      },
      "build:storybook": {
        "outputs": [
          "{workspaceRoot}/build/storybook"
        ]
      },
      "fix": {
        "dependsOn": [
          "^build"
        ]
      },
      "lint": {
        "dependsOn": [
          "^build"
        ]
      },
      "test": {
        "dependsOn": [
          "type-check"
        ]
      },
      "type-check": {
        "dependsOn": [
          "^build"
        ]
      }
    }
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  }
}
